<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.digitalcardscanner">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission. ACCESS_COARSE_LOCATION" />

    <application
        android:hardwareAccelerated="true"
        android:allowBackup="true"
        android:icon="@drawable/logos"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DigitalCardScanner">

        <activity
            android:name="com.example.contactapp.ImportActivity"
            android:label="Import Contact"
            android:exported="true" />
        <activity
            android:name="com.example.contactapp.ContactViewActivity"
            android:label="View Contact"
            android:exported="true"
            android:parentActivityName="com.example.contactapp.MainActivityimportcontact">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.contactapp.MainActivityimportcontact"/>
        </activity>
        <activity
            android:name="com.example.contactapp.ContactDetailsActivity"
            android:exported="true"
            android:excludeFromRecents="true"
            android:launchMode="singleInstance"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat"
            android:label="Contact Details"/>
        <activity
            android:name="com.example.contactapp.AddContactActivity"
            android:exported="true"
            android:parentActivityName="com.example.contactapp.MainActivityimportcontact"
            android:label="Add Contact">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.contactapp.MainActivityimportcontact"/>
        </activity>
        <activity
            android:name="com.example.contactapp.MainActivityimportcontact"
            android:exported="true">
        </activity>


        <activity
            android:name=".AddNewcategory"
            android:exported="true" />
        <activity
            android:name=".CreateCardActivity"
            android:exported="true" />
        <activity android:name=".Add_View_Activity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />


        <activity android:name="com.example.bcmaker.savedActivity"
            android:exported="true"/>
        <activity android:name="com.example.bcmaker.cardActivity"
            android:exported="true"/>
        <activity android:name="com.example.bcmaker.panelActivity"
            android:exported="true"/>
        <activity android:name="com.example.bcmaker.menuActivity"
            android:exported="true">
        </activity>
        <activity
            android:name="com.example.bcmaker.MainActivity"
            android:exported="true"/>


        <activity
            android:name="com.example.qrcodescanner.Activity.LaunchScreenActivity"
            android:exported="true">
        </activity>
        <activity
            android:name="com.example.qrcodescanner.Activity.HistoryActivity"
            android:exported="true" />
        <activity
            android:name="com.example.qrcodescanner.Activity.MainActivity"
            android:exported="true" />

        <activity android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:name="com.example.digitalvisitingcard.DisplayContacts"
            android:exported="true"
            android:label="@string/title_activity_display_contacts"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"/>
        <activity
            android:name="com.example.digitalvisitingcard.ReaderActivity"
            android:exported="true"
            android:label="@string/title_activity_reader"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"/>
        <activity
            android:name="com.example.digitalvisitingcard.ChangingActivity"
            android:exported="true">

        </activity>
        <activity
            android:name="com.example.digitalvisitingcard.AllContacts"
            android:exported="true" />
        <activity
            android:name="com.example.digitalvisitingcard.ActualContact"
            android:exported="true"
            android:label="@string/title_activity_actual_contact"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"/>
        <activity
            android:name="com.example.digitalvisitingcard.MainActivitytransfer"
            android:exported="true">
        </activity>



    </application>

</manifest>